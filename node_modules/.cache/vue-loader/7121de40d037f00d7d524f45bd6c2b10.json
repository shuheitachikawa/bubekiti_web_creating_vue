tle';\n@import '../scss/mixin/contactBtn';\n\n\n$blue:#64D5FF;\n$purple:#8400B5;\n$grey-back:#F4F4F4;\n$black-font:#333333;\n$grey-font:#A0A0A0;\n\n\n.contact{\n  background-image: url('../assets/contact.jpg');\n  background-size: cover;\n  color:white;\n  &__container{\n    @include _container;\n    padding-top: 40px;\n    display: flex;\n    justify-content: space-between;\n  }\n  &__title{\n    @include _sectionTitle('CONTACT');\n    &:after{\n      @include _title-bar;\n    }\n  }\n  &__exp{\n    margin-top: 24px;\n    font-size: 1.6rem;\n  }\n  &__site-name{\n    margin-top: 16px;\n    font-size: 2.8rem;\n  }\n  &__email{\n    margin-top: 16px;\n    font-size: 1.6rem;\n  }\n  .contact-btn{\n    margin-top: 24px;\n    margin-bottom: 48px;\n    @include _contactBtn(64px, 360px);\n    padding-left: 24px;\n    &:before{\n      margin-right: 80px;\n    }\n  }\n}\n\n@media (max-width:1000px){\n  .contact{\n    .contact-btn{\n      @include _contactBtn(56px, 250px);\n    }\n  }\n}\n\n@media (max-width:750px){\n  .contact{\n    &__container{\n      display: block;\n    }\n    &__right{\n      margin-top: 48px;\n    }\n  }\n}\n\n\n</style>\n"]}]}