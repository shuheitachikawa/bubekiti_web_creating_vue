le:#8400B5;\n$grey-back:#F4F4F4;\n$black-font:#333333;\n$grey-font:#A0A0A0;\n\n\n.flow{\n  @include _container;\n  max-width: 730px;\n  text-align: center;\n  position: relative;\n  padding: 35px;\n  &__image{\n    width: 500px;\n    margin: 0 auto;\n  }\n  &__exp{\n    margin-top: 40px;\n    font-size: 1.6rem;\n    text-align: left;\n    line-height: 2.5rem;\n  }\n  &__list{\n    margin: 40px auto 0;\n    text-align: left;\n  }\n  &__item{\n    background-color: $grey-back;\n    padding: 24px 40px;\n    border-radius: 20px;\n    margin-bottom: 80px;\n    position: relative;\n    &:after{\n      content:'';\n      display: block;\n      position: absolute;\n      left:50%;\n      bottom:-130px;\n      height: 170px;\n      width: 33px;\n      border-top: 85px solid transparent;\n      border-bottom: 85px solid transparent;\n      border-left: 33px solid $grey-back;\n      transform: rotate(90deg);\n    }\n    &:last-child{\n      margin-bottom: 24px;\n      &:after{\n        display: none;\n      }\n    }\n  }\n  &__title{\n    font-size: 2rem;\n    font-weight: bold;\n    margin-bottom: 16px;\n  }\n  &__text{\n    font-size: 1.6rem;\n    padding-left: 8px;\n    line-height: 2.7rem;\n  }\n  &__comment{\n    text-align: left;\n    font-size: 1.6rem;\n  }\n  &__btn-area{\n    margin-top: 120px;\n    text-align: center;\n    /deep/.quote__btn{\n      margin: 0 auto;\n    }\n    /deep/.qa__btn{\n      margin-top: 40px;\n    }\n  }\n}\n\n\n</style>\n"],"sourceRoot":"src/views"}]}